{
  "name": "contributune",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "prepare": "lefthook install && npx playwright install chromium",
    "dev": "run-s clean && vite",
    "build": "run-s clean && tsc --project tsconfig.build.json && vite build",
    "clean": "run-p clean:*",
    "clean:dist": "rimraf dist",
    "fmt": "biome format --write .",
    "lnt": "biome lint --write .",
    "che": "biome check --write .",
    "preview": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest watch --silent",
    "test:ci": "vitest run --silent"
  },
  "dependencies": {
    "@1natsu/wait-element": "^4.0.0",
    "lucide-react": "^0.439.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-jsx": "^5.1.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.0.270",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-test-renderer": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser": "^2.1.0",
    "happy-dom": "^15.7.4",
    "lefthook": "^1.7.15",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.47.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-arraybuffer": "^0.0.8",
    "vitest": "^2.1.0"
  }
